{
  "name": "urequest",
  "version": "0.0.6",
  "description": "zero dependency http request wrapper",
  "main": "index.js",
  "scripts": {
    "test": "mocha test --recursive",
    "lint": "eslint .",
    "cover": "nyc --reporter=text-lcov npm test",
    "coveralls": "npm run cover && nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "http",
    "request"
  ],
  "author": "Conor Turner",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-u": "0.0.4",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "urest": "^3.4.4"
  },
  "eslintIgnore": [
    "test/*"
  ],
  "eslintConfig": {
    "extends": "u",
    "rules": {
      "no-else-return": "off"
    }
  }
}
